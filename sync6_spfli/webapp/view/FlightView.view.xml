<mvc:View controllerName="sync6.c1.sync6spfli.controller.FlightView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
    xmlns:u="sap.ui.unified"
	xmlns:c="sap.ui.core">
    <Page id="page" title="{i18n>title}">
        <Panel >
            <VBox >
                <t:Table rows="{/FlightSet}"
                        selectionMode="None"
                        paste="onPaste"
                        ariaLabelledBy="Flight Master">
                    <t:extension>
                        <OverflowToolbar style="Clear">
                            <Title id="flight" text="Flight List" />
                        </OverflowToolbar>
                    </t:extension>
                    <t:columns>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Carrid/@sap:label}"/>
                            <t:template>
                                <Text text="{Carrid}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Connid/@sap:label}"/>
                            <t:template>
                                <Text text="{Connid}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Countryfr/@sap:label}"/>
                            <t:template>
                                <Text text="{Countryfr}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Cityfrom/@sap:label}"/>
                            <t:template>
                                <Text text="{Cityfrom}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Airpfrom/@sap:label}"/>
                            <t:template>
                                <Text text="{Airpfrom}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Countryto/@sap:label}"/>
                            <t:template>
                                <Text text="{Countryto}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Cityto/@sap:label}"/>
                            <t:template>
                                <Text text="{Cityto}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Airpto/@sap:label}"/>
                            <t:template>
                                <Text text="{Airpto}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Fltime/@sap:label}"/>
                            <t:template>
                                <Text text="{Fltime}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Deptime/@sap:label}"/>
                            <t:template>
                                <Text text="{Deptime}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Arrtime/@sap:label}"/>
                            <t:template>
                                <Text text="{Arrtime}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Distance/@sap:label}"/>
                            <t:template>
                                <Text text="{Distance}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Distid/@sap:label}"/>
                            <t:template>
                                <Text text="{Distid}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Fltype/@sap:label}"/>
                            <t:template>
                                <Text text="{Fltype}" wrapping="false" />
                            </t:template>
                        </t:Column>
                        <t:Column width="11rem">
                            <Label text="{/#FlightSet/Period/@sap:label}"/>
                            <t:template>
                                <Text text="{Period}" wrapping="false" />
                            </t:template>
                        </t:Column>
                    </t:columns>
                </t:Table>
            </VBox>
        </Panel>
    </Page>
</mvc:View>